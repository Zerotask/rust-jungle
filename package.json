		"prettier": "~2.3.0",
		"prettier-plugin-svelte": "^2.2.0",
		"svelte-check": "^1.5.4",
		"svelte-preprocess": "^4.7.1",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0",
		"vercel": "^22.0.1",
		"vite-imagetools": "^3.6.7",
		"xmlbuilder2": "^2.4.1"
