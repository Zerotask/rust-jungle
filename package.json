		"workbox-cacheable-response": "^6.1.5",
		"workbox-expiration": "^6.1.5",
		"workbox-routing": "^6.1.5",
		"workbox-strategies": "^6.1.5"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "next",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"autoprefixer": "^10.2.5",
		"cssnano": "^5.0.1",
		"dotenv": "^10.0.0",
		"eslint": "^7.22.0",
