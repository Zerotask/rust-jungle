		"check": "svelte-check",
		"deploy": "vercel",
		"generate": "node ./generator.js"
	},
	"dependencies": {
		"@lukeed/uuid": "^2.0.0",
		"@sveltejs/kit": "next",
		"cookie": "^0.4.1",
